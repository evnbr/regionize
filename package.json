{
  "name": "regionize",
  "version": "1.0.0",
  "description": "Flow HTML through multiple regions",
  "main": "dist/regionize.cjs.js",
  "module": "dist/regionize.esm.js",
  "browser": "dist/regionize.umd.js",
  "unpkg": "dist/regionize.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/evnbr/regionize.git"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@types/jest": "^23.3.14",
    "@types/node": "^13.13.35",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^25.5.4",
    "prettier": "^1.19.1",
    "rollup": "^2.16.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^6.1.0",
    "tslib": "^1.14.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build": "tsc",
    "package": "rollup -c",
    "postbuild": "npm run package && npm run clean",
    "test": "jest",
    "coverage": "jest --coverage",
    "clean": "rimraf build",
    "test-travis": "jest && codecov"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "keywords": [
    "regions",
    "pagination"
  ],
  "author": {
    "name": "Evan Brooks",
    "url": "https://evanbrooks.info"
  },
  "license": "MIT"
}
